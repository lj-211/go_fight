#include "utils/time_util.h"

#include <sys/time.h>
#include <stdio.h>

namespace util {
namespace time {

unsigned long get_u_time(void) {
	struct timeval t;
	int ret = gettimeofday(&t, NULL);
	if (ret == -1) {
		return -1;
	}

	return t.tv_sec * 1000000 + t.tv_usec;
}

} // end namespace time
} // end namespace util

